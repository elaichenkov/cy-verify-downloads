  "description": "Cypress custom command to wait and verify that file has been downloaded",
  "files": [
    "src/index.d.ts",
    "index.js",
    "is-file-exist.plugin.js"
  ],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "cy:open": "npx cypress open",
